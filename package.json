{
  "name": "q-posts",
  "version": "1.0.0",
  "description": "Application for managing posts",
  "main": "index.ts",
  "scripts": {
    "requirements": "solidarity",
    "dev": "webpack serve --config webpack/webpack.config.dev.cjs --color",
    "test": "jest --config jest/jest.config.cjs",
    "build": "rimraf dist && webpack --config webpack/webpack.config.prod.cjs --progress --color",
    "start": "serve ./dist/ -s -p 3000",
    "prettier": "prettier \"*.{json,css,js,ts,tsx}\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "lint": "eslint \"./src/*.{js,ts,tsx}\"",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "release": "semantic-release"
  },
  "author": "Alen Vlahovljak",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "semantic-release": "^21.0.2",
    "solidarity": "^3.0.4",
    "typescript": "^5.2.2",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alenvlahovljak/q-posts.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
